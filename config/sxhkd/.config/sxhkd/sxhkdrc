super + ctrl + s
  pkill -USR1 -x sxhkd
  notify-send "SXHKD reloaded"

ctrl + alt + c
  brave-browser

ctrl + alt + v
  code

super + ctrl + t
   notify-send "Test" "Hallo"


super + z
   rofi -show ssh

super + d 
  rofi -modi drun,run -show drun

super + x
  rofi -show power-menu -modi power-menu:/home/maurice/.local/bin/rofi-power-menu
