super + ctrl + s
  pkill -USR1 -x sxhkd
  notify-send "SXHKD reloaded"

ctrl + alt + c
  brave-browser

ctrl + alt + v
  code

super + ctrl + t
   notify-send "Test" "Hallo"

super + z
  rofi -show ssh

super + space 
  rofi -modi drun,run -show drun

super + x
  rofi -show "Power menu" -modi "Power menu":"/home/maurice/.local/bin/rofi-power-menu"

super + c
  rofi -modi "Config":"/home/maurice/.local/bin/rofi-json /home/maurice/.menus/config.json" -show "Config"

super + m
  mko-menu