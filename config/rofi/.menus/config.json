[
  {
    "name": "journalctl -xeaf",
    "command": "$TERMINAL -e bash -c 'journalctl -xeaf'"
  },
  {
    "name": "udevadm monitor",
    "command": "$TERMINAL -e bash -c 'udevadm monitor --kernel --udev'"
  },
  {
    "name": "dmesg --ctime -w",
    "command": "$TERMINAL -e bash -c 'dmesg --ctime -w'"
  },
  {
    "name": "neofetch",
    "command": "$TERMINAL -e bash -c 'neofetch;read'"
  },
  {
    "name": "edit /etc/fstab",
    "command": "$EDITOR /etc/fstab"
  },
  {
    "name": "edit /efi/EFI/ubuntu/grub.cfg",
    "command": "code -n /efi/EFI /efi/EFI/ubuntu/grub.cfg"
  },
  {
    "name": "edit /etc/X11/xorg.conf",
    "command": "code -n /etc/X11 /etc/X11/xorg.conf"
  },
  {
    "name": "edit /usr/share/applications",
    "command": "code /usr/share/applications"
  },
  {
    "name": "edit ~/.config/rofi",
    "command": "code ~/.config/rofi"
  },
  {
    "name": "open /usr/share/applications",
    "command": "$FILEMANAGER /usr/share/applications"
  }
]